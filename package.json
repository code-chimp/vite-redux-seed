{
  "name": "vite-redux-seed",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "description": "Base React+Redux project with pre-configured static analysis",
  "keywords": [
    "react",
    "redux",
    "redux-toolkit",
    "typescript",
    "vite",
    "template"
